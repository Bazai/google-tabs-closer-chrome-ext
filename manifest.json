{
  "manifest_version": 2,

  "name": "Google search tabs closer",
  "description": "This extension closes all google search tabs.",
  "version": "0.1",

  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}